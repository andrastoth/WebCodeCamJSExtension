{
    "manifest_version": 2,
    "author": "Tóth András",
    "name": "WebCodeCamJS Extension",
    "short_name": "WCCJSEXT",
    "description": "This extension will able to decode qr and barcodes.",
    "version": "1.0.0",
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "css/images/icon16-gray.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "css/images/icon16.png",
        "48": "css/images/icon48.png",
        "128": "css/images/icon128.png"
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["js/content.js"],
        "run_at": "document_end"
    }],
    "background": {
        "page": "background.html",
        "script": "js/DecoderWorker.js"
    },
    "permissions": ["tabs", "<all_urls>", "storage", "debugger"],
